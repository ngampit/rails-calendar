<p id="notice"><%= notice %></p>



<div class="card">
  <div class="card-header">
    Featured
  </div>
  <div class="card-body">
    <h5 class="card-title">Meeting Detail</h5>
    <p>
      <strong>Title:</strong>
      <%= @meeting.name %>
    </p>
    
    <p>
      <strong>Start time:</strong>
      <%= @meeting.start_time %>
    </p>
    
    <p>
      <strong>End time:</strong>
      <%= @meeting.end_time %>
    </p>
    <br></br>
    <button type="button" class="btn btn-warning"><%= link_to 'Edit', edit_meeting_path(@meeting) %> </button>
  </div>
</div>


